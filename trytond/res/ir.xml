            <field name="arch" type="xml">
                <![CDATA[
                    <data>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_install&quot;]"
                            position="replace">
                            <button string="Mark for Installation" name="button_install"
                                type="object" states="{'invisible': '''state != 'uninstalled' ''', 'readonly': ''' %(res.group_admin)d not in groups '''}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_install_cancel&quot;]"
                            position="replace">
                            <button string="Cancel Installation" name="button_install_cancel"
                                type="object" states="{'invisible': '''state != 'to install' ''', 'readonly': ''' %(res.group_admin)d not in groups '''}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_uninstall&quot;]"
                            position="replace">
                            <button string="Mark for Uninstallation (beta)" name="button_uninstall"
                                type="object" states="{'invisible': '''state != 'installed' ''', 'readonly': ''' %(res.group_admin)d not in groups or True'''}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_uninstall_cancel&quot;]"
                            position="replace">
                            <button string="Cancel Uninstallation" name="button_uninstall_cancel"
                                type="object" states="{'invisible': '''state != 'to remove' ''', 'readonly': ''' %(res.group_admin)d not in groups '''}"/>
                        </xpath>
                        <xpath
